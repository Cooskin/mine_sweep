/* pages/index.wxss */
.index {
  width: 100%;
  height: 100vh;
  display: grid;
  place-content: center;
}
.index .bg {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
}

.index .wrap {
  height: 520rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}

.index .wrap .btn_item {
  background-color: #8eb8f7;
  color: #fff;
}